{
  "name": "unofficial-ravensburger-playhub-mcp",
  "version": "1.1.1",
  "description": "MCP server for finding Disney Lorcana TCG events",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "unofficial-ravensburger-playhub-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node --test dist",
    "test:coverage": "c8 --exclude='**/test/**' --reporter=text --reporter=lcov node --test dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "lorcana",
    "tcg",
    "events"
  ],
  "author": "",
  "license": "Unlicense",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zammitt/unofficial-ravensburger-playhub-mcp.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "c8": "^10.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
